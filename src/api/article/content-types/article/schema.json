{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.paragraph-block",
        "blocks.paragraph-with-image-block",
        "blocks.hero-section-2",
        "blocks.heading-block",
        "blocks.full-image-block"
      ]
    },
    "categories": {
      "type": "enumeration",
      "enum": [
        "Sinari Project",
        "GAWIREA Lab",
        "GAWIREA Edu Hub",
        "Solar Sister",
        "Mojuk",
        "Net Zero Heroes"
      ]
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "elements.link"
    }
  }
}
